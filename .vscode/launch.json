{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug App via src/index.js",
      "program": "${workspaceFolder}/src/index.js",
      "console": "integratedTerminal",
      "runtimeExecutable": "node",
      "env": {
        "NODE_ENV": "development"
      },
      "skipFiles": ["<node_internals>/**"]
    }
  ]
}
